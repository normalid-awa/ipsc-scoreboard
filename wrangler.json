{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "ipsc-scoreboard",
	"main": "apps/web/dist/server/server.js",
	"compatibility_date": "2025-09-24",
	"compatibility_flags": [
		"nodejs_compat",
		"nodejs_compat_populate_process_env",
		"allow_custom_ports"
	],
	"placement": { "mode": "smart" },
	"assets": {
		"binding": "ASSETS",
		"directory": "apps/web/dist/client"
	},
	"build": {
		"watch_dir": "app/web/*"
	},
	"observability": {
		"enabled": true,
	},
	"vars": {
		"VITE_BACKEND_API_URL": "http://di.dragoncustom.top:3001"
	},
	"keep_vars": true
}
