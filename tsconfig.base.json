{
	"compilerOptions": {
		"incremental": true,
		"target": "es2019",
		"declaration": true,
		"sourceMap": true,
		"composite": true,
		"strict": true,
		"moduleResolution": "bundler",
		"module": "esnext",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}
